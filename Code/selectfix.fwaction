<item-action name="com.wdstudio.selectfix" title="selectfix"><action-version version="0.1">“selectfix” ActionWalter Davis Studio 2007</action-version><action-javascript>	function fwBeforeEndBody()	{		var select = fwDocument.fwTags.fwFind("select", fwItem);		if (!select)			return;		//alert(fwItem);		var opts = select.fwFindAll("option");		for (i in opts){			if(opts[i].value == undefined) opts[i].value = fwQuote("");			opts[i].label = fwQuote(opts[i].fwFindContent());		}	}	</action-javascript></item-action><page-action name="com.wdstudio.selectfix-page" title="selectfix-page"><action-version version="0.1">“selectfix-page” ActionWalter Davis Studio 2007</action-version><action-javascript>	function fwBeforeEndBody()	{		var selects = fwDocument.fwTags.fwFindAll("select");		if (!selects)			return;		for (i in selects){			//alert(fwItem);			var opts = selects[i].fwFindAll("option");			for (j in opts){				if(opts[j].value == undefined) opts[j].value = fwQuote("");				var lab = opts[j].fwFindContent();				opts[j].label = fwQuote(opts[i].fwFindContent());			}		}	}	</action-javascript></page-action>